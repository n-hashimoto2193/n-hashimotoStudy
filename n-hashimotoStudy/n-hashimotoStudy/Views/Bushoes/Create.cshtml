@model n_hashimotoStudy.Models.Busho

@{
    ViewData["Title"] = "部署情報　登録";
}

<h2>部署情報　登録</h2>

@*<h4>Busho</h4>*@
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="BushoName" class="control-label"></label>
                <input asp-for="BushoName" class="form-control" />
                <span asp-validation-for="BushoName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="登録" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">部署マスターに戻る</a>
</div>

